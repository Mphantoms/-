/**index.wxss**/
.container{
  padding: 0 20rpx;
}
.navTop{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #ccc;
}
.uleft{
  width: 140rpx;
  height: 60rpx;
  background: #FFB800;
  float: left;
  margin-top: 10rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  border-radius: 5px;
}
.uright{
  width: 30%;
  float: right;
  text-align: right;
}
.uright text:last-child{
  color: red;
}
.bodyfz{
  overflow: hidden;
  margin-bottom: 10rpx;
}
.fzleft{
  width: 49%;
  float: left;
  border-right:2px solid #ccc; 
}
.checkbox{
  width: 33%;
  float: left;
  padding: 10rpx 0;
}
.fzright{
  width: 49%;
  float: left;
}
.top{
  height: 60rpx;
  border-bottom: 1px solid #eee;
  margin-bottom: 20rpx;
}
.hbuttom{
  overflow: hidden;
  border-bottom: 1px solid #eee;
  margin-bottom: 10rpx;
}
.hbuttom .radio{
  float: left;
  width: 30%;
  padding-bottom: 20rpx;
}
.myimg{
  height: 300rpx;
  text-align: center;
  border-bottom: 1px solid #eee;
  background: url("data:image/jpg;base64,") no-repeat center center;
}
.imgchoose{
  height: 180rpx;
  width: 100%;
  border-bottom: 1px solid #eee;
  margin-bottom: 10rpx;
}
.imgchoose .imgbox{
  width: 20%;
  margin: 1.5% 2.5%;
  height: 90%;
  float: left;
}
.imgchoose .imgbox image{
  width: 100%;
  height: 100%;
}
.left{
  width: 25%;
  float: left;
  text-align: right;
}
.right{
  width: 65%;
  float: left;
  margin-left: 5%;
}
.right input{
  height: 100rpx;
  border: 1px solid #eee;
  padding-left: 20rpx;
  border-radius: 10rpx;
}
.right textarea{
  border: 1px solid #eee;
  padding-left: 20rpx;
  border-radius: 10rpx;
  max-width: 100%;
}
.right picker{
  border: 1px solid #eee;
  height: 100rpx;
  padding-left: 20rpx;
}
.right .radio{
  width: 50%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  float: left;
}
.row{
  overflow: hidden;
  line-height: 100rpx;
  margin-top: 20rpx;
}
.row:last-child{
  padding-bottom: 50rpx;
}